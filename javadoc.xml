        	notree="false"
        	packagenames="maze.*"
        	source="1.6"
        	sourcepath="src"
        	splitindex="true"
        	use="true"
        	version="true"
        	linksource="true"
        	additionalparam="-notimestamp"
        />
    </target>
</project>
