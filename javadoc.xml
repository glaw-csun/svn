<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="javadoc">
    <target name="javadoc">
        <javadoc
        	access="private"
        	author="true"
        	classpath="lib/jython.jar;lib/rsyntaxtextarea.jar"
        	destdir="javadoc"
        	doctitle="Micro Mouse"
        	nodeprecated="false"
        	nodeprecatedlist="false"
        	noindex="false"
